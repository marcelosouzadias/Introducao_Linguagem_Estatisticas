print("Exercício 1.2")
print("a 10 &plus; 20 × 30")
print("b 42 ÷ 30")
print("c (94 &plus; 2) × 6 − 1")
print("-------------------------------------------------------------------------------")
print()
print("Respostas")
print("a 10 &plus; 20 × 30")
print("-------------------------------------------------------------------------------")
print("b 4² ÷ 30")
conta = 4**2 / 30
print(conta)
print("-------------------------------------------------------------------------------")

